package bar

pred vIsOne(v int) {
    v == 1
}

requires acc(c.SendChannel(), 1/2)
requires c.SendGivenPerm() == vIsOne!<_!>;
requires c.SendGotPerm() == PredTrue!<!>;
func send1(c chan<- int) {
    v := 1
    fold vIsOne!<_!>(v)
    c <- v
}
